{
  "name": "bitsserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon  node ./bin/www",
    "inspect": "node --inspect-brk ./bin/www",
    "build": "cd BitsClient && npm install && npm run build",
    "client": "cd BitsClient && npm start",
    "server": "node ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "engines": {
    "node": "v14.15.4"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  }
}
